{
    "name": "kiiya/chuckjokes",
    "description": "Chuckjokes is a php package that adds Chuck Jokes to your php application",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "kiiya",
            "email": "kiiyaerick@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Kiiya\\ChuckJokes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kiiya\\ChuckJokes\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kiiya\\ChuckJokes\\ChuckJokesServiceProvider"
            ],
            "aliases": {
                "Chuck": "Kiiya\\ChuckJokes\\Facades\\Chuck"
            }
        }
    }
}
